{
    "name": "ironshark/typo3-configuration-manager",
    "type": "library",
    "description": "CLI tools for interacting with Typo3 configuration file LocalConfiguration.php. Can be used to update some serialized extension configs during deployment processes.",
    "keywords": [
        "ironshark",
        "typo3-configuration-manager"
    ],
    "homepage": "https://github.com/ironshark/typo3-configuration-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Pauli",
            "email": "pauli@ironshark.de",
            "homepage": "https://github.com/ironsharkde",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "symfony/console": "^3.0",
        "riimu/kit-phpencoder": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "IronShark\\Typo3ConfigurationManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ironshark\\typo3-configuration-manager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
